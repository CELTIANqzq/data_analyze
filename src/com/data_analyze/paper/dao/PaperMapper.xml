<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.data_analyze.paper.dao.PaperMapper">

	<resultMap id="paperResultMap" type="Paper">

		<id property="_id" column="_id"/>

		<result property="salary_id"  column="salary_id"/>

		<result property="ids_num" column="ids_num"/>

		<result property="year" column="year"/>

		<result property="author" column="author"/>

		<result property="level" column="level"/>

		<result property="department" column="department"/>

		<result property="age" column="age"/>

		<result property="title" column="title"/>

	</resultMap>

	<!--<select id="selectAccount" parameterType="Integer" resultMap="accountResultMap">-->


	<!--</select>-->


	<insert id="insert2015" parameterType="Paper">

		insert into papers2015(salary_id,ids_num,year,author,level,department,age,title)

		values(#{salary_id},#{ids_num},#{year},#{author},#{level},#{department},#{age},#{title});

	</insert>

	<insert id="insert2016" parameterType="Paper">

		insert into papers2016(salary_id,ids_num,year,author,level,department,age,title)

		values(#{salary_id},#{ids_num},#{year},#{author},#{level},#{department},#{age},#{title});

    </insert>

</mapper>